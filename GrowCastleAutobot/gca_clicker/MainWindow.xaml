<Window x:Class="gca_clicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gca_clicker"
        xmlns:ctrl="clr-namespace:gca_clicker"
        mc:Ignorable="d"
        xmlns:cmd="clr-namespace:gca_clicker.Classes.Tooltips"
        Title="GCA" Height="865" Width="390" MinWidth="390" MinHeight="50">
    <!-- height = 865-->
    <Window.Resources>
        <Style x:Key="WhiteBackgroundButton" TargetType="Button">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border BorderThickness="1" BorderBrush="Gray" Background="{TemplateBinding Background}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#FFBAEAFF"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Grid>
        <ScrollViewer Name="AdvancedTabScrollViewer" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible">
            <TabControl x:Name="MyTabControl" PreviewMouseWheel="MyTabControl_PreviewMouseWheel" Margin="7,5,7,7">
                <TabItem Header="Settings" MinWidth="75">

                    <Grid>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="105"/>
                            <!--control-->
                            <RowDefinition Height="92"/>
                            <!--window name, build-->
                            <RowDefinition Height="270"/>
                            <!--dungeon-->
                            <RowDefinition Height="405"/>
                            <!--ab-->
                            <RowDefinition Height="75"/>
                            <!--skip-->
                            <RowDefinition Height="55"/>
                            <!--background-->
                            <RowDefinition Height="80"/>
                            <!--captcha-->
                            <RowDefinition Height="80"/>
                            <!--upgrade hero/castle-->
                            <RowDefinition Height="115"/>
                            <!--ad-->
                            <RowDefinition Height="75"/>
                            <!--altars-->
                            <RowDefinition Height="55"/>
                            <!--replay-->
                            <RowDefinition Height="405"/>
                            <!--screenshots-->
                        </Grid.RowDefinitions>

                        <Canvas Grid.Row="0" Height="1490" VerticalAlignment="Top" Grid.RowSpan="12" Grid.ColumnSpan="2">

                            <Button x:Name="SaveSettingsButton" Style="{StaticResource WhiteBackgroundButton}"
                                    cmd:TooltipHelper.EnabledTooltip="Save settings to file"
                                     cmd:TooltipHelper.DisabledTooltip="Cannot save in runtime"
                                    Click="SaveSettingsButton_Click" Width="25" Canvas.Left="149" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image IsEnabled="{Binding IsEnabled, ElementName=SaveSettingsButton}"
                                       cmd:TooltipHelper.EnabledTooltip="Save settings to file"
                                       cmd:TooltipHelper.DisabledTooltip="Cannot save in runtime"
                                    Source="/Images/Save.png" Width="23" Height="23"/>
                            </Button>

                            <Button x:Name="LoadSettingsButton" Style="{StaticResource WhiteBackgroundButton}"
                                    cmd:TooltipHelper.EnabledTooltip="Load setting from file"
                                      cmd:TooltipHelper.DisabledTooltip="Cannot load in runtime"
                                    Click="LoadSettingsButton_Click" Width="25" Canvas.Left="179" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image IsEnabled="{Binding IsEnabled, ElementName=LoadSettingsButton}"
                                       cmd:TooltipHelper.EnabledTooltip="Load setting from file"
                                       cmd:TooltipHelper.DisabledTooltip="Cannot load in runtime"
                                    Source="/Images/Load.png" Width="20" Height="20"/>
                            </Button>


                            <Button Style="{StaticResource WhiteBackgroundButton}" cmd:TooltipHelper.EnabledTooltip="Open in explorer" Click="OpenInExplorer_Click" Width="25" Canvas.Left="209" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image cmd:TooltipHelper.EnabledTooltip="Open in explorer"
                                    Source="/Images/Explorer.png" Width="20" Height="20"/>
                            </Button>


                            <Button Style="{StaticResource WhiteBackgroundButton}" cmd:TooltipHelper.EnabledTooltip="Open log file" Click="OpenLogFile_Click" Width="25" Canvas.Left="239" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image cmd:TooltipHelper.EnabledTooltip="Open log file"
                                    Source="/Images/File.png" Width="23" Height="23"/>
                            </Button>


                            <Button Padding="0" cmd:TooltipHelper.EnabledTooltip="Leave a star!" cmd:TooltipHelper.TooltipCursor="Hand"
                                    Style="{StaticResource WhiteBackgroundButton}" Click="OpenGithub_Click" Width="25" Canvas.Left="269" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image  cmd:TooltipHelper.EnabledTooltip="Leave a star!" cmd:TooltipHelper.TooltipCursor="Hand"
                                    Source="/Images/Github.png" Width="23" Height="23"/>

                            </Button>

                            <Button Padding="0" cmd:TooltipHelper.EnabledTooltip="Open guide"
                                    Style="{StaticResource WhiteBackgroundButton}" Click="OpenManual_Click" Width="25" Canvas.Left="299" Canvas.Top="10" Height="25" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image  cmd:TooltipHelper.EnabledTooltip="Open guide"
                                    Source="/Images/Question.png" Width="20" Height="20"/>

                            </Button>
                            <Label Name="VersionLabel" Content="Version: 01.01.2001 00:00" FontSize="11.2" Canvas.Left="10" Canvas.Top="10" HorizontalAlignment="Left" VerticalAlignment="Center" Width="139"/>

                            <Button x:Name="StartButton" Style="{StaticResource ControlButton}" Click="StartButton_Click" Width="50" Canvas.Left="16" Canvas.Top="45"  Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image Source="Images/Start.png" Stretch="Uniform" Cursor="Hand"/>
                            </Button>

                            <Image cmd:TooltipHelper.EnabledTooltip="?" cmd:TooltipHelper.TooltipCursor="Wait"
                                    Source="/Images/OldVersion.png" Width="600" Height="594" Canvas.Left="1364"/>
                            <Image cmd:TooltipHelper.EnabledTooltip="?" cmd:TooltipHelper.TooltipCursor="None"
                                    Source="/Images/Scarecrow.png" Width="142" Height="144" Canvas.Left="1873" Canvas.Top="1475" HorizontalAlignment="Center" VerticalAlignment="Top">

                                <Image.RenderTransform>
                                    <ScaleTransform ScaleX="-1"/>
                                </Image.RenderTransform>
                            </Image>

                            <Button x:Name="StopButton" Style="{StaticResource ControlButton}" Click="StopButton_Click" IsEnabled="False" Width="50" Canvas.Left="74" Canvas.Top="45"  Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image Source="/Images/Stop.png" Stretch="Uniform" Cursor="Hand"/>
                            </Button>

                            <Label Name="ThreadStatusLabel" Content="Stopped" Canvas.Left="137" Canvas.Top="50" HorizontalAlignment="Left" VerticalAlignment="Center" Width="187"/>
                            <Label Name="ThreadStatusShortcutLabel" Content="To start:" Canvas.Left="137" Canvas.Top="75" HorizontalAlignment="Center" VerticalAlignment="Top" Width="187"/>


                        </Canvas>



                        <Canvas Grid.Row="1" Height="1490" VerticalAlignment="Top" Grid.RowSpan="11" Grid.ColumnSpan="2">
                            <Separator Canvas.Left="5" Canvas.Top="0" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>


                            <TextBox Name="WindowName" HorizontalAlignment="Left"
                                      cmd:TooltipHelper.DisabledTooltip="Window name without nox version"
                                      cmd:TooltipHelper.EnabledTooltip="Window name without nox version"
                                     Text="NoxPlayer" VerticalAlignment="Top" Width="142" Canvas.Left="100" Canvas.Top="28" Height="25" 
                 VerticalContentAlignment="Center" TextWrapping="NoWrap" TextChanged="TextBox_RewriteSettings"/>

                            <Label Content="Window name:" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="8" Canvas.Top="28" Height="25"/>
                            <Button Content="Set pos" HorizontalAlignment="Center" VerticalAlignment="Top" Click="SetPosButtonClick" Width="78" Canvas.Left="246" Canvas.Top="28" Height="25"/>


                            <CheckBox x:Name="DisableResetCleanupCheck" Content="Disable reset and cleanup buttons check"
                                      cmd:TooltipHelper.EnabledTooltip="Check only if you have reset and cleanup buttons in correct place,&#0010;but script still doesn't allow to start"
                                      Canvas.Left="385" Canvas.Top="34" HorizontalAlignment="Left" VerticalAlignment="Top" Checked="FarmDungeonCheckbox_Checked" Unchecked="FarmDungeonCheckbox_Unchecked"/>


                            <Label Content="Build to play:" Canvas.Left="8" Canvas.Top="62" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <ComboBox Name="BuildToPlayComboBox" PreviewMouseWheel="ComboBox_PreviewMouseWheel" Canvas.Left="100" Canvas.Top="64" Width="54" HorizontalAlignment="Center" VerticalAlignment="Top" SelectionChanged="BuildToPlayComboBox_SelectionChanged">
                                <ComboBoxItem Content="1" />
                                <ComboBoxItem Content="2" />
                                <ComboBoxItem Content="3" />
                                <ComboBoxItem Content="4" />
                                <ComboBoxItem Content="5" />
                            </ComboBox>


                            <Button Padding="0" cmd:TooltipHelper.EnabledTooltip="Go to build"
                                    Click="OpenCurrentBuild_Click" Width="25" Canvas.Left="162" Canvas.Top="63" Height="25" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Image
                                      cmd:TooltipHelper.EnabledTooltip="Go to build"
                                    Source="/Images/Goto.png" Width="17" Height="17" Margin="0,0.5,0,0"/>

                                <Button.Style>
                                    <Style TargetType="{x:Type Button}">
                                        <Setter Property="Background" Value="Transparent"/>
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Button}">
                                                    <Border BorderThickness="1" BorderBrush="#FFacacac" Background="{TemplateBinding Background}">
                                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="#FFBAEAFF"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>



                        </Canvas>




                        <Canvas Grid.Row="2" VerticalAlignment="Top" Height="0" Grid.ColumnSpan="2">
                            <Separator Canvas.Left="5" Canvas.Top="0" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="FarmDungeonCheckbox" Content="Farm dungeon:" Canvas.Left="15" Canvas.Top="30" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="FarmDungeonCheckbox_Checked" Unchecked="FarmDungeonCheckbox_Unchecked"/>

                            <ComboBox x:Name="DungeonComboBox" PreviewMouseWheel="ComboBox_PreviewMouseWheel" IsEnabled="False" Canvas.Left="128" Width="167" HorizontalAlignment="Center" VerticalAlignment="Center" SelectionChanged="DungeonComboBox_SelectionChanged" Canvas.Top="26">
                                <ComboBoxItem Content="Green dragon" />
                                <ComboBoxItem Content="Black dragon" />
                                <ComboBoxItem Content="Red dragon" />
                                <ComboBoxItem Content="Sin" />
                                <ComboBoxItem Content="Legendary dragon" />
                                <ComboBoxItem Content="Bone dragon" />
                                <ComboBoxItem Content="Beginner dungeon" />
                                <ComboBoxItem Content="Intermediate dungeon" />
                                <ComboBoxItem Content="Expert dungeon" />
                            </ComboBox>

                            <Label Name="MatLabel" IsEnabled="False" Content="Mat" Canvas.Left="10" Canvas.Top="59" HorizontalAlignment="Center" VerticalAlignment="Top"/>

                            <CheckBox x:Name="MatBCheckbox" IsEnabled="False" Content="B" Canvas.Left="58" Canvas.Top="64" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatBCheckbox_Checked" Unchecked="MatBCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatACheckbox" IsEnabled="False" Content="A" Canvas.Left="101" Canvas.Top="64" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatACheckbox_Checked" Unchecked="MatACheckbox_Unchecked"/>
                            <CheckBox x:Name="MatSCheckbox" IsEnabled="False" Content="S" Canvas.Left="144" Canvas.Top="64" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatSCheckbox_Checked" Unchecked="MatSCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatLCheckbox" IsEnabled="False" Content="L" Canvas.Left="187" Canvas.Top="64" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatLCheckbox_Checked" Unchecked="MatLCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatECheckbox" IsEnabled="False" Content="E" Canvas.Left="230" Canvas.Top="64" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatECheckbox_Checked" Unchecked="MatECheckbox_Unchecked"/>

                            <Button Style="{StaticResource WhiteBackgroundButton}" cmd:TooltipHelper.EnabledTooltip="Open dragon statistics file" Click="OpenDungeonStatistics_Click" Width="24" Canvas.Left="300" Canvas.Top="25" Height="24" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Image cmd:TooltipHelper.EnabledTooltip="Open dragon statistics file"
                                    Source="/Images/File.png" Width="22" Height="22"/>
                            </Button>

                            <Label Name="MatTimeLabel" IsEnabled="False" Content="Mat, get click delay min (ms):"  Canvas.Top="92" Height="26" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="9"/>
