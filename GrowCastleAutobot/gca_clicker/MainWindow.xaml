<Window x:Class="gca_clicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gca_clicker"
        xmlns:ctrl="clr-namespace:gca_clicker"
        mc:Ignorable="d"
        xmlns:cmd="clr-namespace:gca_clicker.Classes.Tooltips"
        Title="GCA" Height="865" Width="390" MinWidth="390" MinHeight="50">
    <!-- height = 865-->

    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible">
            <TabControl x:Name="MyTabControl" PreviewMouseWheel="MyTabControl_PreviewMouseWheel" Margin="7,5,7,7">
                <TabItem Header="Settings" MinWidth="75">

                    <Grid>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="175"/>
                            <RowDefinition Height="156"/>
                            <RowDefinition Height="54"/>
                            <RowDefinition Height="94"/>
                            <RowDefinition Height="208"/>
                            <RowDefinition Height="55"/>
                            <RowDefinition Height="73"/>
                            <RowDefinition Height="80"/>
                            <RowDefinition Height="115"/>
                            <RowDefinition Height="64"/>
                            <RowDefinition Height="369"/>
                        </Grid.RowDefinitions>

                        <Canvas Grid.Row="0" Height="1490" VerticalAlignment="Top" Grid.RowSpan="11" Grid.ColumnSpan="2">


                            <TextBox Name="WindowName" HorizontalAlignment="Left" Text="NoxPlayer" VerticalAlignment="Center" Width="142" Canvas.Left="101" Canvas.Top="21" Height="25" 
                 VerticalContentAlignment="Center" TextWrapping="NoWrap" TextChanged="TextBox_RewriteSettings"/>

                            <Label Content="Window name:" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="10" Canvas.Top="21" Height="25"/>
                            <Button Content="Set pos" HorizontalAlignment="Left" VerticalAlignment="Center" Click="SetPosButtonClick" Width="76" Canvas.Left="248" Canvas.Top="21" Height="25"/>

                            <Button Content="Save" Click="SaveSettingsButton" Width="76" Canvas.Left="167" Canvas.Top="59" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top"/>

                            <Button Content="Load" Click="LoadSettingsButton" Width="76" Canvas.Left="248" Canvas.Top="59" Height="25" HorizontalAlignment="Center" VerticalAlignment="Top"/>

                            <Label Content="Build to play:" Canvas.Left="10" Canvas.Top="59" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <ComboBox Name="BuildToPlayComboBox" PreviewMouseWheel="ComboBox_PreviewMouseWheel" Canvas.Left="101" Canvas.Top="61" Width="54" HorizontalAlignment="Left" VerticalAlignment="Center" SelectionChanged="BuildToPlayComboBox_SelectionChanged">
                                <ComboBoxItem Content="1" />
                                <ComboBoxItem Content="2" />
                                <ComboBoxItem Content="3" />
                                <ComboBoxItem Content="4" />
                                <ComboBoxItem Content="5" />
                            </ComboBox>


                            <Button x:Name="StartButton" Style="{StaticResource ControlButton}" Click="StartButton_Click" Width="50" Canvas.Left="16" Canvas.Top="94"  Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image Source="Images/Start.png" Stretch="Uniform" Cursor="Hand"/>
                            </Button>


                            <Button x:Name="StopButton" Style="{StaticResource ControlButton}" Click="StopButton_Click" IsEnabled="False" Width="50" Canvas.Left="74" Canvas.Top="94"  Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">
                                <Image Source="/Images/Stop.png" Stretch="Uniform" Cursor="Hand"/>
                            </Button>

                            <Label Name="ThreadStatusLabel" Content="Stopped" Canvas.Left="137" Canvas.Top="98" HorizontalAlignment="Center" VerticalAlignment="Top" Width="187"/>
                            <Label Name="ThreadStatusShortcutLabel" Content="To start:" Canvas.Left="137" Canvas.Top="119" HorizontalAlignment="Center" VerticalAlignment="Top" Width="187"/>


                        </Canvas>

                        <Canvas Grid.Row="1" VerticalAlignment="Top" Grid.ColumnSpan="2">
                            <Separator Canvas.Left="5" Canvas.Top="-27" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="FarmDungeonCheckbox" Content="Farm dungeon:" Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="FarmDungeonCheckbox_Checked" Unchecked="FarmDungeonCheckbox_Unchecked"/>

                            <ComboBox x:Name="DungeonComboBox" PreviewMouseWheel="ComboBox_PreviewMouseWheel" IsEnabled="False" Canvas.Left="128" Canvas.Top="-4" Width="196" HorizontalAlignment="Center" VerticalAlignment="Top" SelectionChanged="DungeonComboBox_SelectionChanged">
                                <ComboBoxItem Content="Green dragon" />
                                <ComboBoxItem Content="Black dragon" />
                                <ComboBoxItem Content="Red dragon" />
                                <ComboBoxItem Content="Sin" />
                                <ComboBoxItem Content="Legendary dragon" />
                                <ComboBoxItem Content="Bone dragon" />
                                <ComboBoxItem Content="Beginner dungeon" />
                                <ComboBoxItem Content="Intermediate dungeon" />
                                <ComboBoxItem Content="Expert dungeon" />
                            </ComboBox>

                            <Label Name="MatLabel" IsEnabled="False" Content="Mat" Canvas.Left="10" Canvas.Top="29" HorizontalAlignment="Center" VerticalAlignment="Top"/>

                            <CheckBox x:Name="MatBCheckbox" IsEnabled="False" Content="B" Canvas.Left="58" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatBCheckbox_Checked" Unchecked="MatBCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatACheckbox" IsEnabled="False" Content="A" Canvas.Left="101" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatACheckbox_Checked" Unchecked="MatACheckbox_Unchecked"/>
                            <CheckBox x:Name="MatSCheckbox" IsEnabled="False" Content="S" Canvas.Left="144" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatSCheckbox_Checked" Unchecked="MatSCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatLCheckbox" IsEnabled="False" Content="L" Canvas.Left="187" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatLCheckbox_Checked" Unchecked="MatLCheckbox_Unchecked"/>
                            <CheckBox x:Name="MatECheckbox" IsEnabled="False" Content="E" Canvas.Left="230" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MatECheckbox_Checked" Unchecked="MatECheckbox_Unchecked"/>

                            <CheckBox x:Name="CastOnBossCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="Works on beginner, intermediate, expert dungeons only.&#0010;Will not activate heros until boss comes out + delay"
                                      cmd:TooltipHelper.EnabledTooltip="Will not activate heros until boss comes out + delay"
                                      IsEnabled="False" Content="Cast on boss" Canvas.Left="15" Canvas.Top="69" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="CastOnBossCheckbox_Checked" Unchecked="CastOnBossCheckbox_Unchecked"/>

                            <Label Name="CastDelayInDungeonLabel" IsEnabled="False" Content="Delay(ms):" Canvas.Left="162" Canvas.Top="63" HorizontalAlignment="Center" VerticalAlignment="Top"/>


                            <TextBox x:Name="CastOnBossDelayTextBox" IsEnabled="False" PreviewTextInput="NumberOnly_PreviewTextInput" Tag="10" Text="500" Width="92" Canvas.Left="230" Canvas.Top="64" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Left" VerticalAlignment="Center" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

                            <CheckBox x:Name="MakeReplaysIfDungeonDoesntLoadCheckBox"
                                      cmd:TooltipHelper.DisabledTooltip="If cannot load dungeons - then until next captcha will replay last wave"
                                      cmd:TooltipHelper.EnabledTooltip="If cannot load dungeons - then until next captcha will replay last wave"
                                      IsEnabled="False" Content="Make replays if dungeon doesn't load" Canvas.Left="15" Canvas.Top="107" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MakeReplaysIfDungeonDoesntLoadCheckBox_Checked" Unchecked="MakeReplaysIfDungeonDoesntLoadCheckBox_Unchecked"/>


                        </Canvas>

                        <Canvas Grid.Row="2" VerticalAlignment="Top" Grid.ColumnSpan="2">

