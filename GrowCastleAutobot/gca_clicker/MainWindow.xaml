                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="SkipWavesCheckbox"
                                      Content="Skip waves"
                                      cmd:TooltipHelper.EnabledTooltip="Will not skip if you have 31, 41, 51, 61, 71, 81, 91 crystals"
                                      Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="SkipWavesCheckbox_Checked" Unchecked="SkipWavesCheckbox_Unchecked"/>
                            <CheckBox x:Name="FiveWavesBetweenSkipsCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="After doing skip, next 5 battles will not skip.&#0010;It was done so that you can activate orc band and military on skip, without losing another crystal (not recommended now).&#0010;Works only in modes where each wave is started manually"
