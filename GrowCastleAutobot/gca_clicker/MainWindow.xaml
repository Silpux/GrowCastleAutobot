                                      IsEnabled="False" Content="Skip with oranges" Canvas.Left="15" Canvas.Top="40" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="SkipWithOrangesCheckbox_Checked" Unchecked="SkipWithOrangesCheckbox_Unchecked"/>



                        </Canvas>

                        <Canvas Grid.Row="4" VerticalAlignment="Top" Grid.ColumnSpan="2">

