                            <Label Name="TimeToBreakABMaxLabel" IsEnabled="False" Content="Max:" Canvas.Left="247" Canvas.Top="159" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <TextBox x:Name="TimeToBreakABMaxTextBox" IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="6" Text="900" Width="45" Canvas.Left="279" Canvas.Top="159" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>



                            <Label Name="SkipsBetweenABSessionsMinLabel" IsEnabled="False" Content="Min:" Canvas.Left="169" Canvas.Top="193" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <TextBox x:Name="SkipsBetweenABSessionsMinTextBox"
                                      cmd:TooltipHelper.DisabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                      cmd:TooltipHelper.EnabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                     IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="2" Text="3" Width="45" Canvas.Left="201" Canvas.Top="193" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

