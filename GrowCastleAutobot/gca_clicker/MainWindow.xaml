                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="ABModeCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Will start wave and enable AB.&#0010;Then will watch game for random amount of seconds between Min and Max from Time to break AB setting, and then wait for first cancel AB button.&#0010;Will restart if lost on AB, or wave is going more than Max battle length"
