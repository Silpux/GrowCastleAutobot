                            <CheckBox x:Name="ABWaveCancelingCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Starts wave, enables AB, and waits for next cancel AB button. Cancel wave, and start again"
                                      Content="AB (wave canceling)" Canvas.Left="129" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="126" Checked="ABWaveCancelingCheckbox_Checked" Unchecked="ABWaveCancelingCheckbox_Unchecked"/>

                            <RadioButton x:Name="GabRadioButton" IsEnabled="False" GroupName="GabTab" Content="Gab" Canvas.Left="15" Canvas.Top="33" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="RadioButton_RewriteSettings" Unchecked="RadioButton_RewriteSettings"/>
                            <RadioButton x:Name="TabRadioButton" IsEnabled="False" GroupName="GabTab" Content="Tab" Canvas.Left="15" Canvas.Top="52" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="RadioButton_RewriteSettings" Unchecked="RadioButton_RewriteSettings"/>

                            <CheckBox x:Name="BreakABOn30CrystalsCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="As soon as 30 white crystals are detected - instead of waiting for AB timer will wait for first cancel AB button.&#0010;Will skip next wave (if Skip waves is enabled)"
                                      cmd:TooltipHelper.EnabledTooltip="Ignores white crystals amount and skips always.&#0010;Will disable automatically when oranges are over"
                                      IsEnabled="False" Content="Break Ab on 30 crystals" Canvas.Left="15" Canvas.Top="85" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="BreakABOn30CrystalsCheckbox_Checked" Unchecked="BreakABOn30CrystalsCheckbox_Unchecked"/>


                            <Label Name="TimeToBreakABLabel" IsEnabled="False" Content="Time to break AB (s):"  Canvas.Top="112" Height="25" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="9"/>

                            <Label Name="ABTimerLabel" Content="" Canvas.Left="175" Canvas.Top="34" HorizontalAlignment="Center" VerticalAlignment="Top"/>


                            <Label Name="TimeToBreakABMinLabel" IsEnabled="False" Content="Min:" Canvas.Left="169" Canvas.Top="112" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox x:Name="TimeToBreakABMinTextBox" IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="6" Text="600" Width="45" Canvas.Left="201" Canvas.Top="112" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Left" VerticalAlignment="Center" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

                            <Label Name="TimeToBreakABMaxLabel" IsEnabled="False" Content="Max:" Canvas.Left="247" Canvas.Top="112" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox x:Name="TimeToBreakABMaxTextBox" IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="6" Text="900" Width="45" Canvas.Left="279" Canvas.Top="112" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Left" VerticalAlignment="Center" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>



                            <Label Name="SkipsBetweenABSessionsMinLabel" IsEnabled="False" Content="Min:" Canvas.Left="169" Canvas.Top="146" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <TextBox x:Name="SkipsBetweenABSessionsMinTextBox"
                                      cmd:TooltipHelper.DisabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                      cmd:TooltipHelper.EnabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                     IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="2" Text="3" Width="45" Canvas.Left="201" Canvas.Top="146" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

                            <Label Name="SkipsBetweenABSessionsMaxLabel" IsEnabled="False" Content="Max:" Canvas.Left="247" Canvas.Top="146" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <TextBox x:Name="SkipsBetweenABSessionsMaxTextBox"
                                      cmd:TooltipHelper.DisabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                      cmd:TooltipHelper.EnabledTooltip="After AB mode is finished (timer ends, or 30 crystals are reached, or lost on ab, or nox freezed) - will do random amount of wave canceling battles between Min and Max before starting next AB session"
                                     IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="2" Text="5" Width="45" Canvas.Left="279" Canvas.Top="146" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>



                            <Label Name="SkipsBetweenABSessionsLabel" IsEnabled="False" Content="Skips between AB sessions:" Canvas.Top="146" Height="25" Canvas.Left="9" HorizontalAlignment="Center" VerticalAlignment="Top"/>




                        </Canvas>

                        <Canvas Grid.Row="5" VerticalAlignment="Top" Grid.ColumnSpan="2">
                            <Separator Canvas.Left="5" Canvas.Top="-27" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox Name="BackgroundModeCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Will not steal cursor, and nox can be overlapped by other windows. But nox cannot be minimized, or changed in size."
                                      Content="Background mode" Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="BackgroundModeCheckbox_Checked" Unchecked="BackgroundModeCheckbox_Unchecked"/>


                        </Canvas>

                        <Canvas Grid.Row="6" Height="1490" VerticalAlignment="Top" Grid.RowSpan="5" Grid.ColumnSpan="2">

                            <Separator Canvas.Left="5" Canvas.Top="-33" Height="16" RenderTransformOrigin="0.5,0.5" Width="304" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="SolveCaptchaCheckbox" Content="Solve captcha" Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="SolveCaptchaCheckbox_Checked" Unchecked="SolveCaptchaCheckbox_Unchecked"/>

                            <CheckBox x:Name="RestartOnCaptchaCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="If you have disabled Solve captcha - then if will hit captcha will restart game, upgrade hero or tower (if enabled), and stop.&#0010;If captcha solving is enabled, and 4 fails are made - will do same (very rare. Captcha solver should solve with 99%+ accuracy)"
                                      Content="Restart on captcha (if 4 fails in row, or solving disabled)" Canvas.Left="15" Canvas.Top="20" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="RestartOnCaptchaCheckbox_Checked" Unchecked="RestartOnCaptchaCheckbox_Unchecked"/>

                        </Canvas>

                        <Canvas Grid.Row="7" VerticalAlignment="Top" Grid.ColumnSpan="2">

                            <Separator Canvas.Left="5" Canvas.Top="-28" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>

                            <CheckBox x:Name="UpgradeCastleCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Will stop upgrading on 9999 lvl"
                                      Content="Upgrade castle for crystals" Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="UpgradeCastleCheckbox_Checked" Unchecked="UpgradeCastleCheckbox_Unchecked"/>



                            <Label Name="FloorToUpgradeCastleLabel" IsEnabled="False" Content="Floor:" Canvas.Top="-7" Height="25" Canvas.Left="211" HorizontalAlignment="Center" VerticalAlignment="Top"/>


                            <ComboBox x:Name="FloorToUpgradeCastleComboBox"
                                      cmd:TooltipHelper.DisabledTooltip="Bottom = 1. Top = 4."
                                      cmd:TooltipHelper.EnabledTooltip="Bottom = 1. Top = 4."
                                      PreviewMouseWheel="ComboBox_PreviewMouseWheel" IsEnabled="False" Canvas.Left="260" Canvas.Top="-4" Width="64" HorizontalAlignment="Center" VerticalAlignment="Top" SelectionChanged="FloorToUpgradeCastle_SelectionChanged">
                                <ComboBoxItem Content="1" />
                                <ComboBoxItem Content="2" />
                                <ComboBoxItem Content="3" />
                                <ComboBoxItem Content="4" />
                            </ComboBox>



                            <CheckBox x:Name="UpgradeHeroForCrystalsCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Will stop upgrading on 9999 lvl"
                                      Content="Upgrade hero for crystals" Canvas.Left="15" Canvas.Top="27" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="UpgradeHeroForCrystalsCheckbox_Checked" Unchecked="UpgradeHeroForCrystalsCheckbox_Unchecked"/>
                            <Label Name="SlotToUpgradeHeroLabel" IsEnabled="False" Content="Slot:" Canvas.Top="21" Height="25" Canvas.Left="211" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <ComboBox x:Name="SlotToUpgradeHeroComboBox"
                                      cmd:TooltipHelper.DisabledTooltip="     1  2  3&#0010;13 4  5  6&#0010;      7  8  9&#0010;   10 11 12"
                                      cmd:TooltipHelper.EnabledTooltip="     1  2  3&#0010;13 4  5  6&#0010;      7  8  9&#0010;   10 11 12"
                                      PreviewMouseWheel="ComboBox_PreviewMouseWheel" IsEnabled="False" Canvas.Left="260" Canvas.Top="23" Width="64" HorizontalAlignment="Center" VerticalAlignment="Top" SelectionChanged="FloorToUpgradeHero_SelectionChanged">
                                <ComboBoxItem Content="1" />
                                <ComboBoxItem Content="2" />
                                <ComboBoxItem Content="3" />
                                <ComboBoxItem Content="4" />
                                <ComboBoxItem Content="5" />
                                <ComboBoxItem Content="6" />
                                <ComboBoxItem Content="7" />
                                <ComboBoxItem Content="8" />
                                <ComboBoxItem Content="9" />
                                <ComboBoxItem Content="10" />
                                <ComboBoxItem Content="11" />
                                <ComboBoxItem Content="12" />
                                <ComboBoxItem Content="13" />
                            </ComboBox>






                        </Canvas>

                        <Canvas Grid.Row="8" VerticalAlignment="Top" Grid.ColumnSpan="2">

                            <Separator Canvas.Left="5" Canvas.Top="-27" Height="16" RenderTransformOrigin="0.5,0.5" Width="305" HorizontalAlignment="Center" VerticalAlignment="Top">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>
                            <CheckBox x:Name="AdForSpeedCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="Ad for speed is watched once per hour. Time of last ad is written in timerx3spd.txt"
                                      Content="Ad for speed" Canvas.Left="15" Canvas.Top="0" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="AdForSpeedCheckbox_Checked" Unchecked="AdForSpeedCheckbox_Unchecked"/>
                            <CheckBox x:Name="AdForCoinsCheckbox"
                                      cmd:TooltipHelper.EnabledTooltip="To quit ad, will just click back button until sees gc.&#0010;If you have troubles with finishing ads, you can set Fixed ad wait in Advanced tab.&#0010;It only quits from ad with back button"
