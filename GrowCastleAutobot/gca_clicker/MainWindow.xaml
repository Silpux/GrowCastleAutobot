                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

                            <Label Name="MatTimeMaxLabel" IsEnabled="False" Content="Max:" Canvas.Left="218" Canvas.Top="93" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            <TextBox x:Name="MatTimeMaxTextBox" IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="6" Text="500" Width="45" Canvas.Left="251" Canvas.Top="93" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>


                            <CheckBox x:Name="CastOnBossCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="Works on beginner, intermediate, expert dungeons only.&#0010;Will not activate heros until boss comes out + delay"
                                      cmd:TooltipHelper.EnabledTooltip="Will not activate heros until boss comes out + delay"
                                      IsEnabled="False" Content="Cast on boss" Canvas.Left="15" Canvas.Top="135" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="CastOnBossCheckbox_Checked" Unchecked="CastOnBossCheckbox_Unchecked"/>

                            <Label Name="CastDelayInDungeonLabel" IsEnabled="False" Content="Delay(ms):" Canvas.Left="187" Canvas.Top="130" HorizontalAlignment="Center" VerticalAlignment="Top" Width="65"/>


                            <TextBox x:Name="CastOnBossDelayTextBox" IsEnabled="False" PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="10" Text="500" Width="45" Canvas.Left="251" Canvas.Top="130" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>

                            <CheckBox x:Name="MakeReplaysIfDungeonDoesntLoadCheckBox"
                                      cmd:TooltipHelper.DisabledTooltip="If cannot load dungeons - then until next captcha will replay last wave"
                                      cmd:TooltipHelper.EnabledTooltip="If cannot load dungeons - then until next captcha will replay last wave"
                                      IsEnabled="False" Content="Make replays if dungeon doesn't load" Canvas.Left="15" Canvas.Top="173" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MakeReplaysIfDungeonDoesntLoadCheckBox_Checked" Unchecked="MakeReplaysIfDungeonDoesntLoadCheckBox_Unchecked"/>

                            <CheckBox x:Name="MissclicksOnDungeonsCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="Make missclicks is normal for human&#0010;If dev can see which dungeon you beat, it will look more human to him.&#0010;Will click wrong dungeon with given chance. Will choose neighbour dungeon"
                                      cmd:TooltipHelper.EnabledTooltip="Make missclicks is normal for human&#0010;If dev can see which dungeon you beat, it will look more human to him.&#0010;Will click wrong dungeon with given chance. Will choose neighbour dungeon"
                                      IsEnabled="False" Content="Missclick on dungeons" Canvas.Left="15" Canvas.Top="205" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MissclickOnDungeonsCheckBox_Checked" Unchecked="MissclickOnDungeonsCheckBox_Unchecked"/>

                            <CheckBox x:Name="MissclicksOnDungeonsIncludeDiagonalsCheckbox"
                                      cmd:TooltipHelper.DisabledTooltip="With it will also missclick on diagonal neighbour dungeons.&#0010;For example, for expert dungeon, legendary dragon is diagonal neighbour,&#0010;so it will missclick to it also"
                                      cmd:TooltipHelper.EnabledTooltip="With it will also missclick on diagonal neighbour dungeons.&#0010;For example, for expert dungeon, legendary dragon is diagonal neighbour,&#0010;so it will missclick to it also"
                                      IsEnabled="False" Content="Include diagonal missclicks" Canvas.Left="15" Canvas.Top="225" HorizontalAlignment="Center" VerticalAlignment="Top" Checked="MissclicksOnDungeonsIncludeDiagonalsCheckBox_Checked" Unchecked="MissclicksOnDungeonsIncludeDiagonalsCheckBox_Unchecked"/>

                            <Label Name="MissclickOnDungeonsChanceLabel" IsEnabled="False" Content="Chance(‰):" Canvas.Left="181" Canvas.Top="199" HorizontalAlignment="Center" VerticalAlignment="Top" Width="74"/>

                            <TextBox x:Name="MissclickOnDungeonsChanceTextBox" IsEnabled="False"
                                      cmd:TooltipHelper.DisabledTooltip="Per mille (1 = 0.1%)"
                                      cmd:TooltipHelper.EnabledTooltip="Per mille (1 = 0.1%)"
                                     PreviewTextInput="NumberOnlyMaxLength_PreviewTextInput" Tag="4" Text="0" Width="44" Canvas.Left="255" Canvas.Top="199" Height="25" 
                                VerticalContentAlignment="Center" TextWrapping="NoWrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextChanged="TextBox_RewriteSettings" LostFocus="TextBox_Insert0OnError"/>


                            <Label Name="DungeonKillSpeedLabel" Content=""  Canvas.Top="245" Height="26" HorizontalAlignment="Left" VerticalAlignment="Center" Canvas.Left="35"/>


